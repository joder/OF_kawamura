load 'common.gpi'
Pr=7
col=12

set ylabel 'T^{+}'

set output 'comp_T7.png'
plot\
  x notitle ls lsT1 \
, KT7 u 2:($3/Pr) w @wDNS t 'Kawamura' ls lsDNS \
, for [i=1:4] srcp(i) u (yp($1)):(subst(column(col))/Pr) w @wMesh t titl(i) ls linstyl(i)

set output 'comp_T7_semilog.png'
