FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilont;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 2 0 0 0];

internalField   uniform 1e-2;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    bottom
    {
        type            epsilontLowReCWTManservisiWallFunction;
        value           uniform 1.6e-3;
    }
    top
    {
        type            epsilontLowReCWTManservisiWallFunction;
        value           uniform 1.6e-3;
    }
    frontAndBack
    {
        type            empty;
    }
}


