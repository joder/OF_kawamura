FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    bottom
    {
        type        kLowReWallFunction;
        value       uniform     1e-10;
    }
    top
    {
        type        kLowReWallFunction;
        value       uniform     1e-10;
    }
    frontAndBack
    {
        type            empty;
    }
}


