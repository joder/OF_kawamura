FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

omegaValue 100;

internalField   uniform $omegaValue;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    bottom
    {
        type        fixedValue;
        value       uniform     $omegaValue;
    }
    top
    {
        type        fixedValue;
        value       uniform     $omegaValue;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
